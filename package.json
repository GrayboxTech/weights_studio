{
  "dependencies": {
    "@protobuf-ts/grpcweb-transport": "^2.11.1",
    "@protobuf-ts/runtime": "^2.11.1",
    "@protobuf-ts/runtime-rpc": "^2.11.1"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.11.1",
    "@types/node": "^24.6.2",
    "typescript": "^4.9.5",
    "vite": "^5.4.11"
  },
  "scripts": {
    "generate-proto": "npm run generate-proto:experiment && npm run generate-proto:data",
    "generate-proto:experiment": "protoc --ts_out src/ --proto_path ../weightslab_ui --ts_opt=generate_dependencies ../weightslab_ui/experiment_service.proto",
    "generate-proto:data": "protoc --ts_out src/ --proto_path ../weightslab_ui --ts_opt=generate_dependencies ../weightslab_ui/data_service.proto",
    "build-client": "tsc src/grpc-client-main.ts --outDir public --target es2020 --module es2020",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate-proto-dead": "protoc --ts_out src/ --proto_path ../weightslab_ui --ts_opt=generate_dependencies ../weightslab_ui/experiment_service.proto"
  }
}
